[run]
source = src/fluxnet_shuttle_lib
omit = 
    */tests/*
    */__pycache__/*
    */.*

[report]
# Fail if coverage is below 100%
fail_under = 100
show_missing = True
skip_covered = False
precision = 2

# Exclude lines from coverage
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod